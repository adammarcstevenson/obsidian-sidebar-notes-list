{
  "type": "module",
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.35.0",
    "@eslint/js": "^9.25.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tsconfig/svelte": "^5.0.4",
    "chalk": "^5.4.1",
    "eslint": "^9.25.0",
    "globals": "^16.0.0",
    "obsidian": "^1.7.2",
    "prettier": "^3.5.3",
    "sass": "^1.70.0",
    "svelte": "^5.16.0",
    "svelte-preprocess": "^6.0.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.0.5",
    "vite-plugin-static-copy": "^2.2.0"
  },
  "scripts": {
    "lint": "eslint .",
    "dev:build": "dotenvx run --env NODE_ENV=development -f .env.development -- vite build --mode development",
    "dev:watch": "dotenvx run --env NODE_ENV=development -f .env.development -- vite build --mode development --watch",
    "prod:build": "dotenvx run --env NODE_ENV=production -f .env.production -- vite build"
  }
}
